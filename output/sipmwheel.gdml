  <solids>
    <box lunit="mm" name="MPPCSolid" x="1.5" y="1.99999997764826" z="1.99999997764826"/>
  </solids>

  <structure>
    <volume name="MPPCLV">
      <materialref ref="G4_Si"/>
      <solidref ref="MPPCSolid"/>
    </volume>
  </structure>

  <setup name="Default" version="1.0">
