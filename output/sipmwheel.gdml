    <matrix coldim="2" name="REFLECTIVITY" values="5e-08 0 1e-06 0 1.5e-06 0 2.25e-06 0 2.481e-06 0 2.819e-06 0 2.952e-06 0 2.988e-06 0 3.024e-06 0 3.1e-06 0 3.14e-06 0 3.1807e-06 0 3.54e-06 0 5.5e-06 0 5.039e-05 0"/>
  </define>

  <materials>
    <isotope N="28" Z="14" name="Si28">
      <atom unit="g/mole" value="27.9769"/>
    </isotope>
    <isotope N="29" Z="14" name="Si29">
      <atom unit="g/mole" value="28.9765"/>
    </isotope>
    <isotope N="30" Z="14" name="Si30">
      <atom unit="g/mole" value="29.9738"/>
